import static org.junit.Assert.*;

import org.junit.Test;


public class TestAdapter {

	@Test
	public void testElgin() {
		Elgin ecf =  new Elgin;
		
		assertEquals(false, ecf.estaAberto());
		ecf.leituraX();
		assertEquals(true, ecf.estAberto());
		ecf.reducaoZ;
		assertEquals(false, ecf.estAberto());
		
		
	}

}
