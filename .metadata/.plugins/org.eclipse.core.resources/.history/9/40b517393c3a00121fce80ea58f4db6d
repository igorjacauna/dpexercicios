
public class CafeSimples implements Cafe {
	
	double preco;
	public CafeSimples() {
		// TODO Auto-generated constructor stub
	}
	
	public CafeSimples(double d) {
		this.preco = d;
	}

	@Override
	public double getPreco() {
		return 1;
	}

	@Override
	public String getIngredientes() {
		return "Cafe";
	}

}
